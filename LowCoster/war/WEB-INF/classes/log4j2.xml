<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<File name="info" fileName="info.log">
			<PatternLayout pattern="%d [%t] %-5level %logger{36} - %msg%n" />
		</File>
	</Appenders>
	<Loggers>
		<Root level="trace">
			<AppenderRef ref="info" level="INFO" />
		</Root>
	</Loggers>
</Configuration>